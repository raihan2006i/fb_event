<div class="page-header">
  <h1><%=t '.title', :default => "Facebook Events" %></h1>
</div>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Title</th>
      <th>Start time</th>
      <th>Location</th>
    </tr>
  </thead>
  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.name %></td>
        <td><%= event.start_time %></td>
        <td><%= event.location %></td>
      </tr>
    <% end %>
  </tbody>
</table>